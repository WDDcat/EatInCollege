<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.eatincollege">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.吃在高校">

        <activity
            android:name=".launcher.LauncherActivity"
            android:launchMode="singleTask"
            android:theme="@style/Theme.AppCompat.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".homepage.HomePageActivity" />
        <activity android:name=".canteen.CanteenActivity" />
        <activity android:name=".community.CommunityActivity" />
        <activity android:name=".mine.MineActivity" />
        <activity android:name=".category.CategoryActivity" />
        <activity android:name=".fooddetail.FoodDetailActivity" />
        <activity android:name=".favourite.FavouriteActivity" />
        <activity android:name=".login.LoginActivity" android:launchMode="singleTask"/>
        <activity android:name=".signup.SignUpActivity" android:launchMode="singleTask" />
<!--        <activity android:name="register"-->
    </application>

</manifest>